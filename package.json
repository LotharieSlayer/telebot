  "bugs": {
    "url": "https://github.com/LotharieSlayer/telebot/issues"
  },
  "homepage": "https://github.com/LotharieSlayer/telebot#readme",
  "dependencies": {
    "enmap": "^5.9.8",
    "telegraf": "^4.12.3-canary.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.7",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "typescript": "^5.1.6"
